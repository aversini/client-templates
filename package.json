{
  "name": "client-template-webpack",
  "description": "Minimal testbed for React with webpack",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build:prod": "webpack -p --env=prod --progress --profile --colors",
    "build": "npm-run-all --serial clean build:prod",
    "clean": "rimraf dist",
    "dev": "webpack-dev-server --env=dev",
    "stats": "npm run clean && npm run build:prod && webpack -p --env=prod --json > build/stats.json && webpack-bundle-analyzer build/stats.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@babel/preset-env": "7.23.3",
    "@babel/preset-react": "7.23.3",
    "babel-loader": "9.1.3",
    "css-loader": "6.8.1",
    "html-webpack-plugin": "5.5.3",
    "mini-css-extract-plugin": "2.7.6",
    "npm-run-all": "4.1.5",
    "style-loader": "3.3.3",
    "url-loader": "4.1.1",
    "webpack": "5.89.0",
    "webpack-bundle-analyzer": "4.9.1",
    "webpack-cli": "5.1.4",
    "webpack-config-utils": "2.3.1",
    "webpack-dev-server": "4.15.1"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "packageManager": "yarn@4.0.1"
}
